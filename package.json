{
  "name": "whollet-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint:eslint": "eslint src/**/*.{ts,tsx}",
    "lint:ts": "tsc && yarn lint:eslint",
    "lint": "yarn lint:ts",
    "format": "prettier --write src/**/*.{ts,tsx} && yarn lint:eslint --fix",
    "svgr-icons": "svgr ./src/assets/icons -d ./src/components/assets/icons --config-file .svgrrc.json --svgo-config .svgorc.json",
    "svgr-illustrations": "svgr ./src/assets/illustrations -d ./src/components/assets/illustrations --config-file .svgrrc.json --svgo-config .svgorc.json"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/app": "^10.2.0",
    "@react-native-firebase/auth": "^10.2.0",
    "@react-native-firebase/database": "^10.2.0",
    "@react-native-firebase/dynamic-links": "^10.2.0",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@shopify/restyle": "^1.3.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "expo": "^53.0.9",
    "react": "19.0.0",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "^3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.10.0",
    "react-native-svg": "15.11.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/cli": "^18.0.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@svgr/cli": "^5.5.0",
    "@types/jest": "^25.2.3",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.2.1",
    "typescript": "^5.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
